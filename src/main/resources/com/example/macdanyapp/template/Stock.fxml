<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.macdanyapp.controllers.StockController">
    <!-- Margen interno del VBox -->
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <ListView fx:id="listViewStockDisponible" prefHeight="700" prefWidth="300"/>
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="buttonAgregarNuevaVajilla" onAction="#buttonAgregarNuevaVajilla" text="Agregar Nueva Vajilla"/>
        <Button text="Volver" fx:id="buttonVolver"  onAction="#buttonVolver" prefWidth="180"/>
    </HBox>


</VBox>
